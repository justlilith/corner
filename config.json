{
  "contentDir":"content"
  , "staticDir":false
  , "publishDir":"src"
  , "mediaTypes": {
    "text/json": {
      "suffixes": ["json"]
    }
  }
  , "disableKinds": [
    "home"
    , "taxonomy"
    , "term"
    , "RSS"
    , "sitemap"
    , "robotsTXT"
    , "404"
  ]
  , "outputFormats": {
    "journal": {
      "name": "journal"
      , "isPlainText": true
      , "isHTML": false
      , "mediaType": "text/json"
      , "notAlternative": true
      , "path": "lib"
    }
    , "work": {
      "isPlainText": true
      , "name": "work"
      , "isHTML": false
      , "mediaType": "text/json"
      , "notAlternative": true
      , "path": "lib"
    }
  }
  ,"uglyURLs": true
  , "outputs": {
    "page": [
    ]
  }
  , "markup": {
    "goldmark": {
      "extensions": {
        "definitionList": true,
        "footnote": true,
        "linkify": true,
        "strikethrough": true,
        "table": true,
        "taskList": true,
        "typographer": true
      },
      "parser": {
        "attribute": {
          "block": false,
          "title": true
        },
        "autoHeadingID": true,
        "autoHeadingIDType": "github"
      },
      "renderer": {
        "hardWraps": false,
        "unsafe": true,
        "xhtml": false
      }
    }
  }
}